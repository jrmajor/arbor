{
    "name": "jrmajor/arbor",
    "description": "",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "arbor",
        "genealogy"
    ],
    "require": {
        "php": "~8.1.0",
        "azjezz/psl": "2.0.0-rc1",
        "doctrine/dbal": "^3.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "jenssegers/agent": "^2.6",
        "jrmajor/laravel-fluent": "^0.1.2",
        "larabug/larabug": "^2.4",
        "laravel/framework": "^8.79",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/telescope": "^4.7",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.1",
        "livewire/livewire": "^2.9",
        "nesbot/carbon": "^2.55",
        "sammyjo20/lasso": "^1.2",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-backup": "^7.7",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-sitemap": "^6.0",
        "symfony/css-selector": "^5.4",
        "symfony/dom-crawler": "^5.4",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.11",
        "facade/ignition": "^2.17",
        "fakerphp/faker": "^1.17",
        "jrmajor/cs": "^0.3.3",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-watch": "1.x-dev",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "helpers.php",
            "app/Services/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
        "post-create-project-cmd": "@php artisan key:generate --ansi"
    }
}
