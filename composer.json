{
    "name": "jrmajor/arbor",
    "description": "Genealogy application built in PHP",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "arbor",
        "genealogy"
    ],
    "require": {
        "php": "~8.3.0",
        "azjezz/psl": "^3.2",
        "guzzlehttp/guzzle": "^7.9",
        "inertiajs/inertia-laravel": "^2.0",
        "jenssegers/agent": "^2.6",
        "jrmajor/laravel-fluent": "^1.2",
        "laravel/framework": "^12.4",
        "laravel/telescope": "^5.6",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.6",
        "league/flysystem-aws-s3-v3": "^3.29",
        "nesbot/carbon": "^3.2",
        "ohdearapp/ohdear-php-sdk": "^3.10",
        "sentry/sentry-laravel": "^4.13",
        "spatie/laravel-activitylog": "dev-fix-tapping#abd79b0",
        "spatie/laravel-flash": "^1.10",
        "spatie/laravel-schedule-monitor": "^3.10",
        "spatie/laravel-sitemap": "^7.3",
        "symfony/css-selector": "^7.0",
        "symfony/dom-crawler": "^7.1",
        "symfony/http-client": "^7.2",
        "symfony/mailgun-mailer": "^7.2",
        "tightenco/ziggy": "^2.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "brianium/paratest": "^7.8",
        "fakerphp/faker": "^1.24",
        "jrmajor/cs": "^0.6.1",
        "larastan/larastan": "^3.2",
        "laravel/pail": "^1.2",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.7",
        "php-standard-library/phpstan-extension": "^2.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-mockery": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.5",
        "spatie/laravel-ignition": "^2.9",
        "vaimo/composer-patches": "^5.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jrmajor/laravel-activitylog"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "vaimo/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patcher": {
            "search": "patches"
        }
    },
    "scripts": {
        "dev" : [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "preview" : [
            "npm run build",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
