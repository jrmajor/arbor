{
	"name": "jrmajor/arbor",
	"description": "Genealogy application built in PHP",
	"license": "MIT",
	"type": "project",
	"keywords": [
		"arbor",
		"genealogy"
	],
	"require": {
		"php": "~8.3.0",
		"azjezz/psl": "^4.2",
		"cweagans/composer-patches": "^2.0",
		"guzzlehttp/guzzle": "^7.10",
		"inertiajs/inertia-laravel": "^2.0",
		"jenssegers/agent": "^2.6",
		"jrmajor/laravel-fluent": "^1.2",
		"laravel/framework": "^12.37",
		"laravel/telescope": "^5.15",
		"laravel/tinker": "^2.10",
		"laravel/ui": "^4.6",
		"league/flysystem-aws-s3-v3": "^3.30",
		"nesbot/carbon": "^3.2",
		"sentry/sentry-laravel": "^4.18",
		"spatie/laravel-activitylog": "^4.10",
		"spatie/laravel-flash": "^1.10",
		"spatie/laravel-schedule-monitor": "^4.1",
		"spatie/laravel-sitemap": "^7.3",
		"symfony/css-selector": "^7.0",
		"symfony/dom-crawler": "^7.3",
		"symfony/http-client": "^7.3",
		"symfony/mailgun-mailer": "^7.3",
		"tightenco/ziggy": "^2.6"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.16",
		"barryvdh/laravel-ide-helper": "^3.6",
		"brianium/paratest": "^7.8",
		"fakerphp/faker": "^1.24",
		"jrmajor/cs": "^0.6.1",
		"larastan/larastan": "^3.8",
		"laravel/pail": "^1.2",
		"mockery/mockery": "^1.6",
		"nunomaduro/collision": "^8.8",
		"php-standard-library/phpstan-extension": "^2.0",
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-mockery": "^2.0",
		"phpstan/phpstan-phpunit": "^2.0",
		"phpunit/phpunit": "^11.5",
		"spatie/laravel-ignition": "^2.9"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		},
		"files": [
			"app/helpers.php",
			"helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"cweagans/composer-patches": true
		},
		"sort-packages": true
	},
	"extra": {
			"patches": {
				"spatie/laravel-activitylog": [
					{
						"description": "fix double tapping",
						"url": "patches/activitylog.patch"
					}
				]
		},
		"laravel": {
			"dont-discover": []
		}
	},
	"scripts": {
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"dev": [
			"Composer\\Config::disableProcessTimeout",
			"pnpx concurrently -k -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"pnpm run dev\" --names=server,queue,logs,vite"
		],
		"preview": [
			"Composer\\Config::disableProcessTimeout",
			"pnpm run build",
			"pnpx concurrently -k -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"pnpm exec php artisan inertia:start-ssr\" --names=server,queue,logs,ssr"
		]
	}
}
