{
    "name": "jrmajor/arbor",
    "description": "Genealogy application built in PHP",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "arbor",
        "genealogy"
    ],
    "require": {
        "php": "~8.3.0",
        "azjezz/psl": "^2.9",
        "doctrine/dbal": "^3.8",
        "guzzlehttp/guzzle": "^7.8",
        "jenssegers/agent": "^2.6",
        "jrmajor/laravel-fluent": "^1.0",
        "larabug/larabug": "^3.0",
        "laravel/framework": "^10.43",
        "laravel/slack-notification-channel": "^3.2",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.4",
        "league/flysystem-aws-s3-v3": "^3.23",
        "livewire/livewire": "^3.4",
        "nesbot/carbon": "^2.72",
        "spatie/laravel-activitylog": "dev-fix-tapping#f85474f",
        "spatie/laravel-backup": "^8.5",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-sitemap": "^7.1",
        "symfony/css-selector": "^7.0",
        "symfony/dom-crawler": "^6.4",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "tightenco/ziggy": "^1.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23",
        "jrmajor/cs": "^0.5.5",
        "larastan/larastan": "^2.8",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.10",
        "php-standard-library/phpstan-extension": "^1.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4",
        "vaimo/composer-patches": "^5.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jrmajor/laravel-activitylog"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "vaimo/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patcher": {
            "search": "patches"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
