{
    "name": "jrmajor/arbor",
    "description": "",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "arbor",
        "genealogy"
    ],
    "require": {
        "php": "~8.1.0",
        "azjezz/psl": "^2.1",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.5",
        "jenssegers/agent": "^2.6",
        "jrmajor/laravel-fluent": "^0.1.5",
        "larabug/larabug": "^2.5",
        "laravel/framework": "^9.41",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/telescope": "^4.9",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "^3.10",
        "livewire/livewire": "^2.10",
        "nesbot/carbon": "^2.63",
        "spatie/laravel-activitylog": "4.4.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-sitemap": "^6.2",
        "symfony/css-selector": "^6.1",
        "symfony/dom-crawler": "^6.1",
        "symfony/mailgun-mailer": "^6.1",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.20",
        "jrmajor/cs": "^0.4.6",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.3",
        "nunomaduro/larastan": "^2.2",
        "php-standard-library/phpstan-extension": "^1.0",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpunit/phpunit": "10.0.x-dev#9caafe2",
        "sebastian/diff": "5.0.x-dev#846e331 as 4.0.4",
        "spatie/laravel-ignition": "^1.6",
        "vaimo/composer-patches": "^5.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php",
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "vaimo/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patcher": {
            "search": "patches"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
